{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/HardVacuumRobot/bin/Debug/netcoreapp3.1/HardVacuumRobot.dll",
            "args": [],
            "cwd": "${workspaceFolder}/HardVacuumRobot",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
